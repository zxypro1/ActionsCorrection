<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.body.BodyActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/toolbar"
        android:minHeight="?attr/actionBarSize"
        android:textAlignment="center"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="体态纠正"
        app:titleTextColor="#FFFFFF" />

    <com.rishabhharit.roundedimageview.RoundedImageView
        android:id="@+id/StandButton"
        android:layout_width="325dp"
        android:layout_height="116dp"
        android:scaleType="centerCrop"
        android:src="@drawable/chmkj13289eiblkzaa0qjq_cl_iaavxagohg_gadso9628"
        app:cornerRadius="17dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        app:layout_constraintVertical_bias="0.53" />

    <com.rishabhharit.roundedimageview.RoundedImageView
        android:id="@+id/imageView17"
        android:layout_width="325dp"
        android:layout_height="116dp"
        android:layout_marginBottom="436dp"
        android:background="#74000000"
        android:scaleType="centerCrop"
        app:cornerRadius="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/StandButton"
        app:layout_constraintEnd_toEndOf="@+id/StandButton"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/StandButton"
        app:layout_constraintTop_toTopOf="@+id/StandButton"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView66"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="站姿"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/StandButton"
        app:layout_constraintEnd_toEndOf="@+id/StandButton"
        app:layout_constraintHorizontal_bias="0.145"
        app:layout_constraintStart_toStartOf="@+id/StandButton"
        app:layout_constraintTop_toTopOf="@+id/StandButton"
        app:layout_constraintVertical_bias="0.265" />

    <TextView
        android:id="@+id/textView67"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="请上传您的站姿照片、视频"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/StandButton"
        app:layout_constraintEnd_toEndOf="@+id/StandButton"
        app:layout_constraintHorizontal_bias="0.336"
        app:layout_constraintStart_toStartOf="@+id/StandButton"
        app:layout_constraintTop_toTopOf="@+id/StandButton"
        app:layout_constraintVertical_bias="0.728" />

